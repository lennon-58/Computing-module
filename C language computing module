#include <stdio.h>

int main() {
    int i;
    int sum = 0;

    for(i = 1; i <= 100; i++) {
        sum = sum + i;
    }

    printf("%d\n", sum);

    return 0;
}

#include <stdio.h>

int main() {
    double count = 0.0;

    while (count <= 100.0) {
        printf("%.2lf\n", count);
        count = count + 0.25;
    }

    return 0;
}

#include <stdio.h>

int main() {
    double count = 0.0;

    while (count >= -100.0) {
        printf("%.1lf\n", count);
        count = count - 0.5;
    }

    return 0;
}

#include <stdio.h>

int main() {
    int i;

    for (i = 0; i <= 10; i++) {
        printf("%d\n", i);
    }

    for (i = 10; i >= 0; i--) {
        printf("%d\n", i);
    }

    return 0;
}

#include <stdio.h>

