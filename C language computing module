#include <stdio.h>

int main() {
    printf("       wWWWw              wWWWw\n");
    printf("vVVVv  (___) wWWWw        (___)  vVVVv\n");
    printf("(___)   ~Y~  (___)  vVVVv  ~Y~   (___)\n");
    printf(" ~Y~    \\|    ~Y~   (___)   |/    ~Y~\n");
    printf(" \\|    \\ |/   \\| /  \\~Y~/  \\|    \\ |/\n");
    printf("\\\\|//  \\\\|// \\\\|/// \\\\|// \\\\|// \\\\\\|///\n");
    printf("^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^\n");

    return 0;
}

#include <stdio.h>

int main() {
    printf("         ((`\\\n");
    printf("      ___ \\\\ '--._\n");
    printf("  .'`    `'    o  )\n");
    printf("  /    \\   '. __.'\n");
    printf(" _|    /_  \\ \\_\\_\n");
    printf("{_\\______\\-'\\__\\_}\n");

    return 0;
}


int main() {
    double a=1;
    double b=2 ;
    double c=3;
    double x;

    x = a + b + c;
    printf("a) x = %f\n", x);

    x=a/b+c;
    printf("b) x=%f\n",x);

    x = a * b * c;
    printf("c) x = %f\n", x);

    x = (a + 1.0) * (b + 2.0) * (c - 10.0);
    printf("d) x = %f\n", x);

    x = (a + b) / c;
    printf("e) x = %f\n", x);
    return 0;
}

#include <stdio.h>

int main() {
    int l = 108;
    int e = 101;
    int n = 110;
    int o = 111;

    printf("My name is: %c%c%c%c%c%c\n", l, e, n, n, o, n);

    return 0;
}

#include <stdio.h>

int main() {
    double time;
    double distance;
    double speed_of_light = 299792458.0;

    printf("How long the space ship has been traveling at warp seed? ");

    scanf("%lf", &time);

    distance = time * speed_of_light;

    printf("The distance traveled is: %lf meters.\n", distance);

    return 0;
}

